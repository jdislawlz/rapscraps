<header>
	<div class="container">
		<div class="row flex-items-md-middle">
			<div class="col-sm-10 offet-sm-1 col-md-3">
				<div class="img-wrap">
					<img src="<%= asset_path('rs-logo.png') %>" alt="Rap Scraps Logo">
				</div>
			</div>
			<nav class="col-sm-1 col-md-9">
				<a href="#" class="info-trigger"><i class="fa fa-info"></i></a>
			</nav>
		</div>
	</div>
</header>
<main>
	<div class="container">
		<div class="row">
			<h1>Rap Scraps</h1>
			<ul>
			<% @tweets.each do |tweet| %>
				<li><%= tweet.full_text %></li>
			<% end %>
			</ul>
		</div>
	</div>
</main>
<footer>
	<div class="container">
		<div class="row">
			<p><strong>Rap Scraps</strong> created by <a href="#">Jose De La Vega</a> and <a href="#">Josh Delgado</a></p>
		</div>
	</div>
</footer>